<h1>Map Type (edit)</h1>

<% form_for(@map_type) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :type %><br />
    <%= f.text_field :type %>
  </p>
  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<hr size="1" color="#ccc">

<p><%= link_to 'Back', :action => 'index' %></p>
